{
  "manifest_version": 3,
  "name": "Bookmark Extension",
  "version": "1.0",
  "description": "Sample Chrome Extension for saving content in db",
  "icons": {
    "16": "assets/16.png",
    "32": "assets/32.png",
    "64": "assets/64.png",
    "128": "assets/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["contextMenus","activeTab"]
}
